{
  "name": "hull-incoming-webhooks",
  "description": "Receive webhooks and enrich your data in Hull",
  "version": "0.1.9",
  "homepage": "https://github.com/hull-ships/hull-incoming-webhooks",
  "license": "MIT",
  "author": "Hull <contact@hull.io> (https://github.com/hull)",
  "engines": {
    "node": "^10.x",
    "npm": "5.x",
    "yarn": "^1.17.x"
  },
  "repository": "git://github.com/hull-ships/hull-incoming-webhooks",
  "bugs": {
    "url": "https://github.com/hull-ships/hull-incoming-webhooks/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors",
    "build:server": "babel server -d lib",
    "start": "node ./lib",
    "start:dev": "NODE_ENV=development LOG_LEVEL=debug nodemon -V -w server -x babel-node -- server",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "test:lint": "eslint server",
    "test:flow": "flow check",
    "test:unit": "NODE_ENV=test mocha --require babel-register -R spec ./test/unit",
    "test:integration": "NODE_ENV=test mocha --require babel-register -R spec ./test/integration",
    "clean": "rimraf dist; rimraf lib",
    "update": "updtr",
    "watch": "watch 'clear && npm run test:lint -s && npm run test:units -s && npm run build:client -s' server src",
    "postinstall": "npm run build",
    "ngrok": "ngrok http 8082 --region eu --subdomain incoming-webhooks"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.7.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.59.3",
    "compression": "^1.7.4",
    "connect": "^3.7.0",
    "connect-timeout": "^1.9.0",
    "css-loader": "^0.28.11",
    "debug": "^3.2.7",
    "deep-diff": "^0.3.8",
    "deep-freeze": "0.0.1",
    "deepmerge": "^1.5.2",
    "domready": "^1.0.8",
    "dotenv": "^4.0.0",
    "event-stream": "3.3.5",
    "express": "^4.17.1",
    "fifo-array": "^0.1.2",
    "hull": "^0.13.23",
    "json-stable-stringify": "^1.0.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongoose": "5.11.17",
    "newrelic": "^7.1.1",
    "raven": "^0.12.3",
    "react": "^15.7.0",
    "react-bootstrap": "^0.33.1",
    "react-dom": "^15.7.0",
    "request": "^2.74.0",
    "response-time": "^2.3.2",
    "rimraf": "^2.7.1",
    "style-loader": "^0.23.1",
    "superagent": "^2.3.0",
    "svg-inline-loader": "^0.8.2",
    "svg-inline-react": "^1.0.3",
    "syntax-error": "^1.4.0",
    "urijs": "^1.19.11",
    "webpack": "^3.12",
    "webpack-dev-middleware": "^2.0.6",
    "winston-logstash": "^0.4.0"
  },
  "devDependencies": {
    "assert": "^1.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.4.1",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.16.0",
    "flow-bin": "^0.145.0",
    "jwt-decode": "^2.2.0",
    "minihull": "^2.1.2",
    "mocha": "^2.5.3",
    "nodemon": "^1.19.4",
    "react-hot-loader": "^1.3.1",
    "sinon": "^1.17.7",
    "supertest": "^1.2.0",
    "updtr": "^0.2.3",
    "watch": "^0.19.2"
  },
  "main": "index.js"
}
