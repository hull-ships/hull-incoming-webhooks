{
  "name": "hull-incoming-webhooks",
  "version": "0.2.0",
  "description": "Receive webhooks and enrich your data in Hull",
  "homepage": "https://github.com/hull-ships/hull-incoming-webhooks",
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-incoming-webhooks"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-incoming-webhooks/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://hull.io"
  },
  "keywords": [
    "hull",
    "ship",
    "connector",
    "hull-ship",
    "hull-connector",
    "hull-connector-template-1.0.0"
  ],
  "engines": {
    "node": "8.11.x",
    "npm": "5.6.x",
    "yarn": "1.6.x"
  },
  "scripts": {
    "ngrok": "ngrok http 8082 --region eu --subdomain incoming-webhooks",
    "build": "yarn run build:clean && yarn run build:server && yarn run build:client",
    "build:clean": "rimraf dist && rimraf build",
    "build:server": "babel server -d build",
    "build:client": "NODE_ENV=production webpack --config ./webpack.config.js",
    "start": "node -r newrelic build",
    "start:dev": "NODE_ENV=development LOG_LEVEL=debug babel-watch -L server",
    "dev": "yarn start:dev",
    "test": "yarn test:lint && yarn test:flow && yarn test:unit && yarn test:integration",
    "test:lint": "eslint server src",
    "test:flow": "flow check",
    "test:unit": "NODE_ENV=test mocha --require babel-register -R spec ./test/unit",
    "test:integration": "NODE_ENV=test mocha --require babel-register -R spec ./test/integration",
    "postinstall": "yarn run flow:install && yarn run build",
    "prettier:client": "prettier --write \"./src/**/*.{js,jsx,json}\"",
    "prettier:server": "prettier --write \"./server/**/*\"",
    "flow": "flow",
    "flow:install": "$(npm bin)/flow-typed install",
    "flow:update": "$(npm bin)/flow-typed update"
  },
  "dependencies": {
    "autoprefixer": "^8.3.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bluebird": "^3.4.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.39.2",
    "connect": "^3.6.6",
    "connect-timeout": "^1.9.0",
    "css-loader": "^0.28.11",
    "debug": "^3.1.0",
    "deep-freeze": "^0.0.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "happypack": "^4.0.1",
    "hull": "0.13.16",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "mongoose": "4.11.11",
    "newrelic": "^4.0.0",
    "node-sass": "^4.9.2",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.4",
    "progress-bar-webpack-plugin": "^1.11.0",
    "proptypes": "^1.1.0",
    "querystring": "^0.2.0",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.0.1",
    "request": "^2.87.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "svg-inline-loader": "^0.8.0",
    "svg-inline-react": "^3.1.0",
    "syntax-error": "^1.4.0",
    "urijs": "^1.19.1",
    "url": "^0.11.0",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^2.0.2",
    "webpack-hot-middleware": "^2.22.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-flowtype-errors": "^3.3.6",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.14.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.69.0",
    "flow-typed": "^2.4.0",
    "jest": "^22.4.2",
    "mini-application": "^1.1.0",
    "minihull": "^3.0.0",
    "mocha": "^5.2.0",
    "nock": "^9.0.14",
    "prettier": "^1.12.1"
  },
  "resolutions": {
    "mini-application/sinon": "6.1.2"
  }
}
