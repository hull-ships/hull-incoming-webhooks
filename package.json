{
  "name": "hull-incoming-webhooks",
  "description": "Run code to map User Properties and generate Events",
  "version": "0.1.2",
  "homepage": "https://github.com/hull-ships/hull-incoming-webhooks",
  "license": "MIT",
  "author": "Hull <contact@hull.io> (https://github.com/hull)",
  "engines": {
    "node": "6.x",
    "npm": "3.x",
    "yarn": "0.27.5"
  },
  "repository": "git://github.com/hull-ships/hull-incoming-webhooks",
  "bugs": {
    "url": "https://github.com/hull-ships/hull-incoming-webhooks/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:client": "NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors",
    "build:server": "babel server -d lib",
    "start": "node ./lib",
    "start:dev": "NODE_ENV=development LOG_LEVEL=debug nodemon -V -w server -x babel-node -- server",
    "test": "npm run test:lint && npm run test:flow && npm run test:unit && npm run test:integration",
    "test:lint": "eslint server",
    "test:flow": "flow check",
    "test:unit": "NODE_ENV=test mocha --require babel-register -R spec ./test/unit",
    "test:integration": "NODE_ENV=test mocha --require babel-register -R spec ./test/integration",
    "clean": "rimraf dist; rimraf lib",
    "update": "updtr",
    "watch": "watch 'clear && npm run test:lint -s && npm run test:units -s && npm run build:client -s' server src",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship"
  ],
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "codemirror": "^5.16.0",
    "connect": "^3.6.3",
    "connect-timeout": "^1.7.0",
    "css-loader": "^0.23.1",
    "deep-diff": "^0.3.4",
    "deep-freeze": "0.0.1",
    "deepmerge": "^1.3.2",
    "domready": "^1.0.8",
    "event-stream": "^3.3.3",
    "express": "^4.14.0",
    "fifo-array": "^0.1.2",
    "hull": "0.12.2",
    "json-stable-stringify": "^1.0.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "mongoose": "4.11.11",
    "newrelic": "^1.28.3",
    "raven": "^0.11.0",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.1.0",
    "request": "^2.74.0",
    "response-time": "^2.3.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "superagent": "^2.0.0",
    "svg-inline-loader": "^0.6.0",
    "svg-inline-react": "^1.0.1",
    "syntax-error": "^1.3.0",
    "urijs": "^1.18.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.12.0",
    "winston-logstash": "^0.2.11"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "flow-bin": "^0.51.1",
    "jwt-decode": "^2.2.0",
    "minihull": "^2.0.1",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "updtr": "^0.1.15",
    "watch": "^0.19.1",
    "webpack": "^1.13.1"
  },
  "main": "index.js"
}
